{
  "$schema": "https://json.schemastore.org/block.json",
  "apiVersion": 2,
  "name": "smt-theme/section",
  "version": "0.1.5",
  "title": "General",
  "category": "block-category-sections",
  "description": "",
  "supports": {
    "html": false,
    "anchor": false,
    "customClassName": false
  },
  "attributes": {
    "topPadding": {
      "type": "number",
      "default": "1"
    },
    "bottomPadding": {
      "type": "number",
      "default": "1"
    },
    "backgroundAlt": {
      "type": "string",
      "default": ""
    },
    "backgroundDefaultAlt": {
      "type": "string",
      "default": ""
    },
    "xlbackgroundImageId": {
      "type": "number"
    },
    "xlbackgroundImagesrcArray": {
      "type": "object"
    },
    "mdbackgroundImageId": {
      "type": "number"
    },
    "mdbackgroundImagesrcArray": {
      "type": "object"
    },
    "xsbackgroundImageId": {
      "type": "number"
    },
    "xsbackgroundImagesrcArray": {
      "type": "object"
    },
    "backgroundColor": {
      "type": "string",
      "default": "#ffffff"
    },
    "backgroundColorClass": {
      "type": "string",
      "default": "one"
    },
    "showLogo": {
      "type": "boolean",
      "default": false
    },
    "LogoColor": {
      "type": "string",
      "default": "light"
    },
    "hideLG": {
      "type": "boolean",
      "default": false
    },
    "hideMD": {
      "type": "boolean",
      "default": false
    },
    "hideXS": {
      "type": "boolean",
      "default": false
    },
    "anchor": {
      "type": "string",
      "default": ""
    },
    "extraClass": {
      "type": "string",
      "default": ""
    }
  },
  "keywords": [
    "section",
    "block"
  ],
  "textdomain": "smt-theme",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css"
}